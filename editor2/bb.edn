{:deps {}
 :tasks
 {dev (shell "yarn shadow-cljs -A:local watch editor")
  server (shell "yarn shadow-cljs -A:local server")
  copy-curriculum (shell "cp -r src/main/maria/curriculum public")
  css (shell "npx tailwindcss -i src/maria.cloud.css -o public/css/maria.cloud.css")
  release (do (run 'copy-curriculum)
              (run 'css)
              (shell "yarn shadow-cljs release editor --debug"))
  debug (shell "yarn shadow-cljs release editor --debug")}}